{
  "name": "else",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:unit": "jest",
    "test:e2e": "playwright test --workers=3",
    "test:e2e:debug": "playwright test --workers=3 --debug",
    "clean:build": "find . -name '.next' -type d -prune -exec rm -rf '{}' +",
    "clean:node-modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "clean:package-lock": "find . -name 'package-lock.json' -type f -prune -exec rm -rf '{}' +",
    "clean:test-results": "find . -name 'test-results' -type d -prune -exec rm -rf '{}' +",
    "clean": "npm run clean:build && npm run clean:node-modules && npm run clean:package-lock && npm run clean:test-results"
  },
  "dependencies": {
    "@playwright/test": "^1.51.1",
    "next": "15.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/text-encoding": "^0.0.40",
    "dotenv": "^16.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.51.1",
    "tailwindcss": "^4",
    "text-encoding": "^0.7.0",
    "ts-jest": "^29.3.1",
    "typescript": "^5",
    "wait-on": "^8.0.3"
  }
}
